{
  "name": "howves",
  "version": "0.0.1-beta.3",
  "description": "a simple Vue Composition utils",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx rollup -c",
    "update:beta-version": "npx ts-node ./scripts/updateVersion.ts beta",
    "update:release-version": "npx ts-node ./scripts/updateVersion.ts release",
    "update:publish": "npx ts-node ./scripts/publish.ts",
    "switch2": "npx vue-demi-switch 2",
    "switch3": "npx vue-demi-switch 3"
  },
  "homepage": "https://github.com/biancangming/howves",
  "repository": {
    "type": "git",
    "url": "git@github.com:biancangming/howves.git"
  },
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    }
  },
  "keywords": [],
  "author": "biancangming",
  "license": "ISC",
  "dependencies": {
    "@vue/composition-api": "^1.4.6",
    "howtools": "^0.2.1-beta.6",
    "vue-demi": "^0.12.1"
  },
  "devDependencies": {
    "@types/node": "^16.9.1",
    "axios": "^0.25.0",
    "consola": "^2.15.3",
    "cross-env": "^6.0.3",
    "esbuild": "^0.14.23",
    "esbuild-register": "^3.0.0",
    "rollup": "^2.66.0",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-esbuild": "^4.8.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.4",
    "vite-svg-loader": "^2.2.0",
    "vue": "^3.0.0"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "vue": "^2.0.0 || >=3.0.0",
    "howtools": "^0.2.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  }
}